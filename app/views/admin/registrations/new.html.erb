<section class="inner">
  <h2 class="section-ttl">新規登録a</h2>

  <%= form_with model: @admin, url: admin_registration_path, id: 'new_user', class: 'new_user', local: true do |f| %>
    <%= render 'layouts/error_messages', resource: @admin %>


    <div class="field mb2">
      <%= f.label :email, "メールアドレス" %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div class="field mb2">
      <%= f.label :password, "パスワード" %><br />
      <%= f.password_field :password, autocomplete: "current-password" %>
    </div>

    <div class="actions">
      <%= f.submit "ログイン", class: "btn btn-primary" %>
    </div>
  <% end %>
</section>

